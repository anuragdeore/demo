pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello World'
            }
        }
         stage('build') {
            steps {
                echo 'project build'
            }
        }
         stage('deployment') {
            steps {
                echo 'deployment start'
            }
        }
        
         stage('testing') {
            steps {
                echo 'testing start'
            }
        }
         stage('release') {
            steps {
                echo 'releases'
            }
        }
    }
}
